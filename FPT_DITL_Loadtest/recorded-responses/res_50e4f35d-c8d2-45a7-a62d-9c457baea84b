jQuery.sap.require("sap.m.Dialog");jQuery.sap.require("sap.m.Text");jQuery.sap.require("sap.m.Button");if("undefined"==typeof businessobjects){var businessobjects={};}businessobjects.webutil=businessobjects.webutil||{};businessobjects.webutil.fioribi=businessobjects.webutil.fioribi||{};businessobjects.webutil.fioribi.dialogservice=businessobjects.webutil.fioribi.dialogservice||{};businessobjects.webutil.fioribi.dialogservice.DialogConstants={TIMEOUT_WARNING_DIALOG:"warnDialog",TIMEOUT_INFO_DIALOG:"infoDialog",ID_PREFIX:"dialogService_",CUSTOM_MESSAGE_DIALOG:"CUSTOM_MESSAGE_DIALOG",NO_CLOSE_MESSAGE_DIALOG:"noCloseMessageDialog"};businessobjects.webutil.fioribi.dialogservice.DialogService={dialogCounter:1,lastDialogId:null,createDialogId:function(){var id=businessobjects.webutil.fioribi.dialogservice.DialogConstants.ID_PREFIX+this.dialogCounter;this.dialogCounter++;this.lastDialogId=id;return id;},showDialog:function(config){if(!config.dialogType&&!config.message){return null;}var id=this.createDialogId();var dialog;if(config.dialogType){switch(config.dialogType){case businessobjects.webutil.fioribi.dialogservice.DialogConstants.TIMEOUT_WARNING_DIALOG:dialog=new sap.m.Dialog(id,{title:config.title,type:"Message",state:"Warning",content:new sap.m.Text({text:config.message}),beginButton:new sap.m.Button({text:config.button_Continue_title,press:config.fnCallbackMessageBox}),endButton:new sap.m.Button({text:config.button_Logoff_title,press:config.fnCallbackMessageBox}),afterClose:function(){dialog.destroy();}});break;case businessobjects.webutil.fioribi.dialogservice.DialogConstants.TIMEOUT_INFO_DIALOG:dialog=new sap.m.Dialog(config.id,{title:config.title,type:"Message",content:new sap.m.Text({text:config.message}),beginButton:new sap.m.Button({text:config.button_OK_title,press:config.fnCallbackMessageBox}),afterClose:function(){dialog.destroy();}});break;case businessobjects.webutil.fioribi.dialogservice.DialogConstants.CUSTOM_MESSAGE_DIALOG:dialog=new sap.m.Dialog(id,{title:config.title,type:"Message",state:"Warning",content:new sap.m.Text({text:config.message}),beginButton:new sap.m.Button({text:config.button_Continue_title,press:config.fnCallbackMessageBox}),afterClose:function(){dialog.destroy();}});break;case businessobjects.webutil.dialogservice.DialogConstants.NO_CLOSE_MESSAGE_DIALOG:dialog=new sap.m.Dialog(id,{title:config.title,type:"Message",content:new sap.m.Text({text:config.message}),afterClose:function(){dialog.destroy();}});break;}return dialog;}}};