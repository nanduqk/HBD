jQuery.sap.declare({modName:"com.sap.fioribi.modules.search.searchBarController",type:"controller"});jQuery.sap.require({modName:"com.sap.fioribi.modules.common.BILPController",type:"controller"});jQuery.sap.require("com.sap.fioribi.modules.search.SearchService");jQuery.sap.require("com.sap.fioribi.utils.ObjectTypeSmallIcon");jQuery.sap.require("com.sap.fioribi.modules.search.SearchConstants");com.sap.fioribi.modules.search.searchBarController=function(){com.sap.fioribi.modules.common.BILPController.apply(this,arguments);};com.sap.fioribi.modules.search.searchBarController.prototype=jQuery.sap.newObject(com.sap.fioribi.modules.common.BILPController.prototype);com.sap.fioribi.modules.search.searchBarController.prototype.onInit=function(){this.searchCallbacks=[];this.focusInCallbacks=[];this.focusOutCallbacks=[];};com.sap.fioribi.modules.search.searchBarController.prototype.addSearchCallback=function(searchCallback){this.searchCallbacks.push(searchCallback);};com.sap.fioribi.modules.search.searchBarController.prototype.callSearchCallbacks=function(searchString,searchFieldFilter,searchDocumentsFilter){for(var i=0;i<this.searchCallbacks.length;i++){this.searchCallbacks[i](searchString,searchFieldFilter,searchDocumentsFilter);}};com.sap.fioribi.modules.search.searchBarController.prototype.addFocusInCallback=function(focusInCallback){this.focusInCallbacks.push(focusInCallback);};com.sap.fioribi.modules.search.searchBarController.prototype.callFocusInCallbacks=function(){for(var i=0;i<this.focusInCallbacks.length;i++){this.focusInCallbacks[i]();}};com.sap.fioribi.modules.search.searchBarController.prototype.addFocusOutCallback=function(focusOutCallback){this.focusOutCallbacks.push(focusOutCallback);};com.sap.fioribi.modules.search.searchBarController.prototype.callFocusOutCallbacks=function(){for(var i=0;i<this.focusOutCallbacks.length;i++){this.focusOutCallbacks[i]();}};com.sap.fioribi.modules.search.searchBarController.prototype.search=function(searchString,searchFieldFilter,searchDocumentsFilter,page,pageSize,callback){if(searchFieldFilter==null){searchFieldFilter=com.sap.fioribi.modules.search.SearchConstants.ALL_KEY;}if(searchDocumentsFilter==null){searchDocumentsFilter=com.sap.fioribi.modules.search.SearchConstants.ALL_DOCUMENTS_KEY;}var getSearchListSuccess=function(response){this.searchResultFetched(response,callback);}.bind(this);var getSearchListError=function(xhr,ajaxOptions,thrownError){console.log("Failed to get Search Results");}.bind(this);var searchStringWithDocumentsFilter=com.sap.fioribi.modules.search.SearchService.buildSearchString(searchString,searchFieldFilter,searchDocumentsFilter);com.sap.fioribi.modules.search.SearchService.search(searchStringWithDocumentsFilter,null,page,pageSize,false,getSearchListSuccess,getSearchListError);};com.sap.fioribi.modules.search.searchBarController.prototype.searchResultFetched=function(searchResult,callback){var list=[];var searchHits=searchResult.hits;for(var i=0,hitCount=searchHits.length;i<hitCount;i++){var searchEntry=searchHits[i];var map={};searchEntry.properties.forEach(function(property){map[property.name]=property.value;});var oIcon=new com.sap.fioribi.utils.ObjectTypeSmallIcon(map.SI_KIND);list.push({cuid:map.SI_CUID,description:map.SI_DESCRIPTION,decoratedDescription:map.SI_HIT_DESCRIPTION_DECORATED,reportEngineType:map.reportEngineType,id:map.SI_ID,name:map.SI_NAME,decoratedTitle:map.SI_HIT_TITLE_DECORATED,ownerid:map.SI_OWNER,owner:map.SI_OWNER,type:map.SI_KIND,updated:map.SI_UPDATE_TS,icon:oIcon,iconURL:oIcon.getSrc()});}var oModel=new sap.ui.model.json.JSONModel();oModel.setData({list:list});callback(oModel);};com.sap.fioribi.modules.search.searchBarController.prototype.processActions=function(oControl,objContext){BILaunchpadApp.oActionPanel.renderActionPanel(oControl,objContext);};