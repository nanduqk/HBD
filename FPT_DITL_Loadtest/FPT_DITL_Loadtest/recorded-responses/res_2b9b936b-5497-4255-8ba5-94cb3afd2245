jQuery.sap.declare("com.sap.fioribi.utils.TableIcon");var types=["Pdf","Txt","Powerpoint","Excel","Word","Agnostic","Hyperlink","SharedElement","Folder","Category","PersonalCategory"];var tableIconType=function(type){var iconType;if(type=="Pdf"){iconType="sap-icon://pdf-attachment";}else{if(type=="Txt"){iconType="sap-icon://attachment-text-file";}else{if(type=="Powerpoint"){iconType="sap-icon://ppt-attachment";}else{if(type=="Excel"){iconType="sap-icon://excel-attachment";}else{if(type=="Word"){iconType="sap-icon://doc-attachment";}else{if(type=="Hyperlink"){iconType="sap-icon://chain-link";}else{if(type=="Agnostic"){iconType="sap-icon://document-text";}else{if(type=="Shortcut"){iconType="sap-icon://shortcut";}else{if(type=="SharedElement"){iconType="sap-icon://share-2";}else{if(type=="Folder"){iconType="sap-icon://folder";}else{if(type=="Category"||type=="PersonalCategory"){iconType="sap-icon://tag";}else{iconType="sap-icon://folder";}}}}}}}}}}}return iconType;};var tableIconColor=function(type){var iconColor;if(type=="Pdf"){iconColor=sap.ui.core.theming.Parameters.get("sapNegativeColor");}else{if(type=="Txt"){iconColor=sap.ui.core.theming.Parameters.get("sapTextColor");}else{if(type=="Powerpoint"){iconColor=sap.ui.core.theming.Parameters.get("sapAccentColor2");}else{if(type=="Excel"){iconColor=sap.ui.core.theming.Parameters.get("sapPositiveColor");}else{if(type=="Word"){iconColor=sap.ui.core.theming.Parameters.get("sapInformativeColor");}else{if(type=="Agnostic"){iconColor=sap.ui.core.theming.Parameters.get("sapNeutralColor");}else{if(type=="Shortcut"){iconColor=sap.ui.core.theming.Parameters.get("sapNeutralColor");}else{if(type=="SharedElement"){iconColor=sap.ui.core.theming.Parameters.get("sapAccentColor6");}else{if(type=="Folder"){iconColor=sap.ui.core.theming.Parameters.get("sapNeutralColor");}else{if(type=="Hyperlink"){iconColor=sap.ui.core.theming.Parameters.get("sapInformativeColor");}else{if(type=="Category"||type=="PersonalCategory"){iconColor=sap.ui.core.theming.Parameters.get("sapInformativeColor");}else{iconColor=sap.ui.core.theming.Parameters.get("sapNeutralColor");}}}}}}}}}}}return iconColor;};