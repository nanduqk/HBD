define("zen.rt.components.tabstrip/resources/js/tab_handler",["sap/zen/basehandler"],function(a){sap.zen.TabHandler=function(){sap.zen.BaseHandler.apply(this,arguments);var b=sap.zen.Dispatcher.instance;var c=this;this.createAndAdd=function(h,k,m,i,l){var e=k.id;var f=this.createDefaultProxy(e);if(sap.zen.designmode&&sap.zen.designmode.isDesignMode(k.id)){f.setEnabled=function(n){f.designmodeEnabled=n;if(n){this.$().removeClass("sapUiTabDsbl");}else{this.$().addClass("sapUiTabDsbl");}};f.getEnabled=function(){return true;};}if(sap.zen.Dispatcher.instance.isMainMode()){f.setText(k.caption);}else{var j=new sap.ui.commons.Title();j.setText(k.caption);f.setTitle(j);}f.setEnabled(k.enabled);var g=this.createAbsoluteLayout();i(f,l);f.addContent(g);d(f,k,m);return f;};this.updateComponent=function(h,f,g,e){if(f){d(h,f,g);}return h;};function d(f,k,l){if(k){var g=f.getContent()[0];var m=k.caption;if(sap.zen.Dispatcher.instance.isMainMode()){if(m!=null){var n=f.getText();if(n!=m){f.setText(m);}}}else{var h=f.getTitle();if(m!=null){var n=h.getText();if(n!=m){h.setText(m);}}}var i=k.enabled;if(i!=null){var j=f.getEnabled();if((sap.zen.designmode&&sap.zen.designmode.isDesignMode(k.id))||j!=i){f.setEnabled(i);}}var e=k.content;if(e){c.updateChildren(e,f,function(t,s,r){if(r){var p=b.createAbsoluteLayoutInfo(r);var q=b.calcWidthUseOrderOfPriority(p.left,p.right,r.width);var o=b.calcHeightUseOrderOfPriority(p.top,p.bottom,r.height);b.setWidthHeight(t,q,o);}b.insertIntoAbsoluteLayoutContainer(g,t,s);},function(o){g.removeContent(o);});}}}this.applyForChildren=function(k,h){var g=k.getContent()[0];var f=g.getContent();for(var e=0;e<f.length;e++){var j=f[e];if(j){h(j);}}};this.getDefaultProxyClass=function(){return["sap.m.IconTabFilter","sap.ui.commons.Tab"];};this.getDecorator=function(){return"TabDecorator";};this.getType=function(){return"tab";};};return new sap.zen.TabHandler();});