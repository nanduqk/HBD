jQuery.sap.declare({modName:"com.sap.fioribi.app.BILaunchpadView",type:"view"});jQuery.sap.require("sap.ui.core.mvc.JSView");com.sap.fioribi.app.BILaunchpadView=function(){sap.ui.core.mvc.JSView.apply(this,arguments);};com.sap.fioribi.app.BILaunchpadView.prototype=jQuery.sap.newObject(sap.ui.core.mvc.JSView.prototype);com.sap.fioribi.app.BILaunchpadView.prototype.getControllerName=function(){return"com.sap.fioribi.app.BILaunchpadController";};com.sap.fioribi.app.BILaunchpadView.prototype.createContent=function(oController){var mainApp=new sap.m.App();jQuery.sap.require("com.sap.fioribi.modules.homepage.HomepageContainer");var homePage=new com.sap.fioribi.modules.homepage.HomepageContainer();BILaunchpadApp.setHomePage(homePage);mainApp.addPage(homePage.getUI());var isCatCallback=findCallback("callbackGetisCat");if(isCatCallback!=null){var isCat=isCatCallback();if(isCat&&isCat!="null"){var startFolderCallback=findCallback("callbackGetStartFolder");var startFolder;if(startFolderCallback!=null){startFolder=startFolderCallback();}if(startFolder&&startFolder!="null"&&isCat=="false"){var myHomePageSpace=sap.ui.getCore().byId("tabPage").getContent()[0].getSections()[0].getSubSections()[0].getBlocks()[0];myHomePageSpace.handlePressEvent(null,myHomePageSpace.getController(),"Folders");}else{if(startFolder&&startFolder!="null"&&isCat=="true"){var myHomePageSpace=sap.ui.getCore().byId("tabPage").getContent()[0].getSections()[0].getSubSections()[0].getBlocks()[0];myHomePageSpace.handlePressEvent(null,myHomePageSpace.getController(),"Categories");}else{this.pagePreference();}}}else{this.pagePreference();}}else{this.pagePreference();}var invokeCallback=findCallback("callbackIsThemeEnabled");var isThemeEnabled=null;if(invokeCallback!=null){isThemeEnabled=invokeCallback();}if(isThemeEnabled!=null&&isThemeEnabled.toLowerCase()=="custom"){BILaunchpadApp.includeStyleSheet("com/sap/fioribi/css/custom-css/Theme.css");}else{BILaunchpadApp.includeStyleSheet("com/sap/fioribi/css/Default.css");}return mainApp;};com.sap.fioribi.app.BILaunchpadView.prototype.pagePreference=function(){var landingPage=BILaunchpadApp.getHomepageTab();if(BILaunchpadApp.getselecthometab()=="customPage"){var techLandingPage="";switch(landingPage){case"alldocuments":techLandingPage="Documents";break;case"schedule":techLandingPage="Schedules";break;case"categories":techLandingPage="Categories";break;case"inbox":techLandingPage="Inbox";break;case"folders":techLandingPage="Folders";break;case"recyclebin":techLandingPage="RecycleBin";break;}var myHomePageSpace=sap.ui.getCore().byId("tabPage").getContent()[0].getSections()[0].getSubSections()[0].getBlocks()[0];myHomePageSpace.handlePressEvent(null,myHomePageSpace.getController(),techLandingPage);}else{if(BILaunchpadApp.getselecthometab()=="customGroup"){var landingsection="";var sectionid;switch(BILaunchpadApp.getHomePageFilter()){case"1":landingsection="myHomeSection";break;case"2":landingsection="favoriteSection";sectionid="5";break;case"3":landingsection="RecentViewedSection";sectionid="2";break;case"4":landingsection="RecentRunSection";sectionid="6";break;case"5":landingsection="pageApplicationSection";break;}if(BILaunchpadApp.getLisitngType()=="tileview"||!sectionid){sap.ui.getCore().byId("tabPage").getContent()[0].setSelectedSection(landingsection);}else{var favorRecentlyViewedspace=sap.ui.getCore().byId(landingsection).getSubSections()[0].getBlocks()[0];favorRecentlyViewedspace.handleFooterPress(null,favorRecentlyViewedspace.getController(),sectionid);}}else{if(BILaunchpadApp.getselecthometab()=="categoriesTab"){var myHomePageSpace=sap.ui.getCore().byId("tabPage").getContent()[0].getSections()[0].getSubSections()[0].getBlocks()[0];myHomePageSpace.handlePressEvent(null,myHomePageSpace.getController(),"Categories");}}}};