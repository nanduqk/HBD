define("zen.rt.components.actionsheet/resources/js/actionsheet_handler",["sap/zen/basehandler"],function(b){var a=function(){b.apply(this,arguments);var c=b.dispatcher;var d=this;this.oPlacementMapping={BOTTOM:sap.m.PlacementType.Bottom,LEFT:sap.m.PlacementType.Left,RIGHT:sap.m.PlacementType.Right,TOP:sap.m.PlacementType.Top,HORIZONTAL:sap.m.PlacementType.Horizontal,VERTICAL:sap.m.PlacementType.Vertical,AUTO:sap.m.PlacementType.Auto,};this.oActionSheet=null;var f=0;var e=function(g,o){var r=function(){if(d.oActionSheet){d.oActionSheet.destroyButtons();d.oActionSheet.destroy();d.oActionSheet=null;}};if(o.open===true){r();d.oActionSheet=new sap.m.ActionSheet(g.getId()+ ++f+"_rendered",{afterClose:function(){r();if(o.closeCommand){new Function(o.closeCommand)();}}});if(o.placement){d.oActionSheet.setPlacement(d.oPlacementMapping[o.placement]);}else{d.oActionSheet.setPlacement(sap.m.PlacementType.Auto);}var s=0;var h=o.items;if(h){var n;var p;var j=o.onClickCommand;for(var k=0;k<h.length;k++){n=h[k].item;p=new sap.m.Button(d.oActionSheet.getId()+"_button_"+k);if(n.text){p.setText(n.text);}else{p.setText(n.value);}var m=n.icon;if((m!==undefined)&&(m.length!=0)){p.setIcon(n.icon);}if((n.isVisible!==undefined)&&(!n.isVisible)){p.setVisible(n.isVisible);s++;}if((n.isEnabled!==undefined)&&(!n.isEnabled)){p.setEnabled(n.isEnabled);}var l=j.replace("__VALUE__",n.value);var q=new Function(l);p.fOnClickCommand=q;p.attachPress(function(){o.closeCommand="";this.fOnClickCommand();});d.oActionSheet.addButton(p);}if(h.length==s){p=new sap.m.Button(d.oActionSheet.getId()+"_button_"+h.length+1);p.setEnabled(false);p.setText("");d.oActionSheet.addButton(p);}}var t=sap.zen.Dispatcher.instance.getRootControlForComponentId(o.openBy);if(t){d.oActionSheet.openBy(t);}}};this.create=function(h,g,j){var k=g.id;var i=this.createButton(k);e(i,g,j);i.setVisible(false);return i;};this.update=function(i,g,h){if(g){e(i,g);}return i;};this.getDefaultProxyClass=function(){return[sap.m.ActionSheet,null];};this.getType=function(){return"actionsheet";};};return new a();});