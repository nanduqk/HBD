define("LayerPaletteBuilder",["../../../../../js/colorPalette/js/MessageBundle"],function(a){var b=function(){this.create=function(c,o,f,k){var d=sap.zen.Dispatcher.instance.dispatchAdvancedPropertyCall(c.renderParams.vizId,"layerProperties");var g=f.layerId;var n={};for(var j=0;j<d.length;++j){var m=d[j];if(m.id===g){n=m;}}var e=null;if(n.type==="Shapes"&&n.measureColorPalette){e=n.measureColorPalette;}else{if(n.dimensionColorPalette){e=n.dimensionColorPalette;}}var h={layerId:f.layerId,layerType:n.type,appliedPalette:e};if(n.type!="Points"){this._colorPaletteWidget=sap.ludsi.ColorPaletteWidget.create(undefined,h);return this._colorPaletteWidget;}else{var l=new sap.m.VBox();var p=new sap.m.Label({text:a.getText("GEO_COLORPALETTES_NOT_SUPPORTED")}).addStyleClass("sapUiTinyMarginBegin").addStyleClass("sapUiTinyMarginEnd");l.addItem(p);return l;}};this.isAutoClose=function(){return false;};};return b;});