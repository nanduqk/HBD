jQuery.sap.declare("com.sap.fioribi.utils.ErrorHandler");com.sap.fioribi.utils.ErrorHandler=function(){this.oActionCallback=null;};com.sap.fioribi.utils.ErrorHandler.prototype.showErrorMessage=function(oResponse,oErrorPlaceHolder,fnCallback){if(oResponse){if(typeof oResponse==="string"){if(oResponse.indexOf("<html>")!==-1){var oHtmlContainer=new sap.ui.core.HTML();oHtmlContainer.setContent(oResponse);oErrorPlaceHolder.addContent(oHtmlContainer);}}else{var errMessage=oResponse.message||oResponse.error;if(!errMessage){if(oResponse.CAF_ERROR&&oResponse.CAF_ERROR.CAF_DISPLAY_TEXT){errMessage=oResponse.CAF_ERROR.CAF_DISPLAY_TEXT;}}var errLbl=new sap.m.Label();errLbl.setText(errMessage);errLbl.setDesign(sap.m.LabelDesign.Bold);oErrorPlaceHolder.addContent(errLbl);}if(fnCallback instanceof Function){fnCallback();}}};com.sap.fioribi.utils.ErrorHandler.prototype.showErrorPopup=function(oResponse,fnCallback){var errMessage=BILaunchpadApp.localized("generic_error_message");if(oResponse.message){errMessage=oResponse.message;}else{if(oResponse.error){if(typeof oResponse.error=="string"){errMessage=oResponse.error;}else{if(oResponse.error.localizedError){errMessage=oResponse.error.localizedError;}}}}jQuery.sap.require("sap.m.MessageBox");sap.m.MessageBox.error(errMessage,{title:BILaunchpadApp.localized("Recycle_Bin_Error"),onClose:null,styleClass:"",initialFocus:null,textDirection:sap.ui.core.TextDirection.Inherit});if(fnCallback instanceof Function){fnCallback();}};