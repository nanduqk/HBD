sap.designstudio.sdk.Component.subclass("com.sap.ip.bi.Timer",function(){var b=this;var a=null;this.init=function(){this.$().css("display","none");};this.runningState=function(d){if(!d){return(a!=null?"START":"STOP");}else{if(d=="START"){if(a!=null){window.clearInterval(a);a=null;}a=window.setInterval(function(){b.fireEvent(["onTimer"]);},this.millis());}else{if(d=="STOP"){if(a!=null){window.clearInterval(a);a=null;}}}return this;}};var c=1000;this.millis=function(d){if(d===undefined){return c;}else{if(d>=0){c=d;}return this;}};});