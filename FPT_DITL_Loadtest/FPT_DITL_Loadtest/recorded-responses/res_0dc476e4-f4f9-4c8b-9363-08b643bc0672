jQuery.sap.declare("com.sap.fioribi.components.actionpanel.ActionPanelContainer");jQuery.sap.require({modName:"com.sap.fioribi.components.actionpanel.ActionPanelView",type:"view"});jQuery.sap.require({modName:"com.sap.fioribi.components.actionpanel.ActionPanelController",type:"controller"});jQuery.sap.require("com.sap.fioribi.components.actionpanel.ActionPanelContainer");var menui="";var globalCAFactions="";com.sap.fioribi.components.actionpanel.ActionPanelContainer=function(oData){this.oView=new com.sap.fioribi.components.actionpanel.ActionPanelView({viewName:"com.sap.fioribi.components.actionpanel.ActionPanelView"});this.oController=this.oView.getController();};com.sap.fioribi.components.actionpanel.ActionPanelContainer.prototype=jQuery.sap.newObject(com.sap.fioribi.modules.common.BILPContainer.prototype);com.sap.fioribi.components.actionpanel.ActionPanelContainer.prototype.renderActionPanel=function(oControl,objContext,parentController,layout){var popover=this.oView.renderActionPanel();menui=this.oView.renderActionPanel();if(popover.getItems().length>0){this.oController.emptyModel();}var objData;if(objContext instanceof Array){objData=[];for(var index=0;index<objContext.length;index++){var bindingItems=objContext[index];if(typeof(bindingItems.getPath)=="function"){objData.push(bindingItems.getProperty(bindingItems.getPath()));}else{objData.push(bindingItems);}}}else{if(typeof(objContext.getPath)=="function"){objData=objContext.getProperty(objContext.getPath());}else{objData=objContext;}}this.oController.setCurrentObject(objData);this.oController.updateActionPanel();this.oController.setParentController(parentController);this.oController.setLayout(layout);if(oControl.sId!="homeTable"&&oControl.sId!="folderTable"&&oControl.sId!="scheduleTable"&&oControl.sId!="recyclebinTable"&&oControl.sId!="historyTable"){if(popover.getItems().length>0){var eDock=sap.ui.core.Popup.Dock;popover.open(false,oControl,eDock.BeginTop,eDock.BeginBottom,oControl);}else{sap.m.MessageToast.show(BILaunchpadApp.localized("Client_Actions_Rights_message"));}}};com.sap.fioribi.components.actionpanel.ActionPanelContainer.prototype.menuit=function(){return menui;};com.sap.fioribi.components.actionpanel.ActionPanelContainer.prototype.globalCAF=function(){return globalCAFactions;};com.sap.fioribi.components.actionpanel.ActionPanelContainer.prototype.performAction=function(objContext,oCafObj,parentController){var objData;if(typeof(objContext.getPath)=="function"){objData=objContext.getProperty(objContext.getPath());}else{objData=objContext;}if(parentController!=undefined){this.oController.setParentController(parentController);}this.oController.setCurrentObject(objData);this.oController.peformAction(oCafObj);};com.sap.fioribi.components.actionpanel.ActionPanelContainer.prototype.renderGlobalActionPanel=function(oControl,objContext,parentController,layout,container){var popover=this.oView.renderActionPanel();globalCAFactions=this.oView.renderActionPanel();if(popover.getItems().length>0){this.oController.emptyModel();}var objData;if(objContext instanceof Array){objData=[];for(var index=0;index<objContext.length;index++){objData.push(objContext[index]);}}else{if(typeof(objContext.getPath)=="function"){objData=objContext.getProperty(objContext.getPath());}else{objData=objContext;}}this.oController.setCurrentObject(objData);this.oController.setCafContainer(container);this.oController.updateGlobalActionPanel();this.oCurrentObject=objData;this.oController.setParentController(parentController);this.oController.setLayout(layout);if(!BILaunchpadApp.getGlobalCAFFlag()){if(popover.getItems().length>0){var eDock=sap.ui.core.Popup.Dock;popover.open(false,oControl,eDock.BeginTop,eDock.BeginBottom,oControl);}}};