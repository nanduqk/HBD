function foldersModel(){var folders=new sap.ui.model.json.JSONModel();this.getFolders=function(){return folders;};this.setFolders=function(input){folders.setData(input);};}function tableModel(){var allItems=new sap.ui.model.json.JSONModel();this.getallItems=function(){return allItems;};this.setallItems=function(input){allItems.setData(input);};}