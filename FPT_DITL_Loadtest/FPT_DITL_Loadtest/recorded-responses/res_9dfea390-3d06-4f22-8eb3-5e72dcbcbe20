function myHomeModel(){var categoryTileList=new sap.ui.model.json.JSONModel([]);var documentTileList=new sap.ui.model.json.JSONModel([]);var favoriteDataList=[];var categoryTile=new sap.ui.model.json.JSONModel({name:null,type:null,count:null,lastRef:null,cuid:null,id:null,description:null});this.getCategoryTile=function(){return categoryTile;};this.setCategoryTile=function(tile){categoryTile.setData(tile);};this.getCategoryTileList=function(){return categoryTileList;};this.setCategoryTileList=function(tilelist){categoryTileList.setData(tilelist);};var documentTile=new sap.ui.model.json.JSONModel({name:null,type:null,count:null,lastRef:null,cuid:null,lastRef:null,description:null,});this.getdocumentTile=function(){return documentTile;};this.setdocumentTile=function(tile){documentTile.setData(tile);};this.getdocumentTileList=function(){return documentTileList;};this.setdocumentTileList=function(tilelist){documentTileList.setData(tilelist);};var recentlyUpdatedList=new sap.ui.model.json.JSONModel([]);var pageInfo=new sap.ui.model.json.JSONModel({cuid:null,name:null,level:0,type:null});this.getpageInfo=function(){return pageInfo;};this.setpageInfo=function(data){pageInfo.setData(data);};var pageInfoList=new sap.ui.model.json.JSONModel([]);this.addpageInfoList=function(data){pageInfoList.getData().push(data);};this.getpageInfoList=function(i){return pageInfoList.getData()[i];};this.removepageInfoList=function(){return pageInfoList.getData().pop();};this.getCountpageInfoList=function(){return pageInfoList.getData().length;};this.getList=function(){return pageInfoList.getData();};var selectedFilter=new sap.ui.model.json.JSONModel({flag:0});this.getselectedFilter=function(){return selectedFilter;};this.setselectedFilter=function(data){selectedFilter.setData(data);};this.getFavoriteDataList=function(){return favoriteDataList;};this.clearFavoriteDataList=function(){favoriteDataList=[];};}