jQuery.sap.declare("com.sap.fioribi.utils.BILaunchpadUtil");com.sap.fioribi.utils.BILaunchpadUtil=function(){};com.sap.fioribi.utils.BILaunchpadUtil.prototype.getClassName=function(sBundlePath){var aClassName=sBundlePath.split(".");var oClassName=(window||this);for(var i=0;i<aClassName.length;i++){oClassName=oClassName[aClassName[i]];}if(typeof oClassName!=="function"){throw new Error("Class Name not found");}return oClassName;};com.sap.fioribi.utils.BILaunchpadUtil.prototype.getBrowserLocale=function(){var language=window.navigator.userLanguage||window.navigator.language;var formattedLocale=language.replace("-","_");return formattedLocale;};com.sap.fioribi.utils.BILaunchpadUtil.prototype.isViewSupported=function(sKind){var aUnsupportedType=["Folder","Category","Xcelsius","Program"];for(i=0;i<aUnsupportedType.length;i++){if(aUnsupportedType[i]==sKind){return false;}}return true;};com.sap.fioribi.utils.BILaunchpadUtil.prototype.isUnifiedViewSupported=function(sKind){var aSupportedType=["Webi","CrystalReport","AAD.AnalysisApplication","MDAnalysis","LUMX","AFDashboardPage","Analytic","ExternalHyperlink","Publication"];for(i=0;i<aSupportedType.length;i++){if(aSupportedType[i]==sKind){return true;}}return false;};com.sap.fioribi.utils.BILaunchpadUtil.prototype.appendParamforSAML=function(url){if(url!=undefined&&url.length>0){if(url.indexOf("?")>0){url=url+"&isFromTopLevelApp=true";}else{url=url+"?isFromTopLevelApp=true";}return url;}return url;};com.sap.fioribi.utils.BILaunchpadUtil.prototype.formatOpenDocURL=function(srcUrl){var OpenDocLink="/OpenDocument/opendoc/openDocument.jsp";var webapproot=document.location.pathname;webapproot=webapproot.substring(0,webapproot.indexOf(getRootContext()));var url=srcUrl;if(url.indexOf(OpenDocLink)>-1){url=url.substring(url.indexOf(OpenDocLink));srcUrl=window.location.protocol+"//"+window.location.host+webapproot+url;}if(appKind=="InfoView"||appKind=="OpenDocument"){srcUrl=com.sap.fioribi.utils.BILaunchpadUtil.prototype.appendParamforSAML(srcUrl);}return srcUrl;};com.sap.fioribi.utils.BILaunchpadUtil.prototype.formatHyperLinkURL=function(surl){if((surl.indexOf("http")===0||surl.indexOf("https")===0)){var browserUrl=document.location.href;var hyperLinkJSURL=new URL(surl);var browserJSUrl=new URL(browserUrl);if(browserJSUrl.protocol==hyperLinkJSURL.protocol&&browserJSUrl.host==hyperLinkJSURL.host){surl=com.sap.fioribi.utils.BILaunchpadUtil.prototype.formatOpenDocURL(surl);}}else{surl=com.sap.fioribi.utils.BILaunchpadUtil.prototype.formatOpenDocURL(surl);}return surl;};