var sBasePath=window.location.pathname.replace(/\/[^\/]*\.?[^\/]*$/,"/");var sBaseUrl=window.location.protocol+"//"+window.location.host+sBasePath;var sResourceBundlePath=sBaseUrl+"i18n/fioribiResources.properties";getResourceBundle=function(Locale){var oCore=sap.ui.getCore();var oModel=new sap.ui.model.resource.ResourceModel({bundleUrl:sResourceBundlePath,bundleLocale:Locale});oCore.setModel(oModel,"i18n");return oModel.getResourceBundle();};