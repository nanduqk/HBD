define(function(){var a={};jQuery.sap.require("sap.m.SegmentedButtonRenderer");jQuery.sap.extend(a,sap.m.SegmentedButtonRenderer);a.render=function(d,c){var n=c.getButtons(),e=c.getSelectedButton(),l,q,m,o;if(c._bInOverflow){d.write("<div");d.writeControlData(c);d.writeClasses();d.write(">");d.renderControl(c.getAggregation("_select"));d.write("</div>");return;}d.write("<ul");if(a._addAllIconsClass(n)){d.addClass("sapMSegBIcons");}d.addClass("sapMSegB");d.writeClasses();if(c.getWidth()&&c.getWidth()!==""){d.addStyle("width",c.getWidth());}d.writeStyles();d.writeControlData(c);q=c.getTooltip_AsString();if(q){d.writeAttributeEscaped("title",q);}d.writeAccessibilityState(c,{role:"radiogroup"});d.write(">");for(var k=0;k<n.length;k++){l=n[k];d.write("<li");if(l.getVisible()){var g=l.getText(),h=l.getIcon(),p="",b;if(h){b=l._getImage((l.getId()+"-img"),h);if(b instanceof sap.m.Image){c._overwriteImageOnload(b);}else{if(!l.getTooltip()){p=c._getIconAriaLabel(b);}}}d.writeControlData(l);d.writeAttribute("aria-posinset",k+1);d.writeAttribute("aria-setsize",n.length);d.addClass("sapMSegBBtn");if(l.aCustomStyleClasses!==undefined&&l.aCustomStyleClasses instanceof Array){for(var f=0;f<l.aCustomStyleClasses.length;f++){d.addClass(l.aCustomStyleClasses[f]);}}if(l.getEnabled()){d.addClass("sapMSegBBtnFocusable");}else{d.addClass("sapMSegBBtnDis");}if(e===l.getId()){d.addClass("sapMSegBBtnSel");}if(h&&g!==""){d.addClass("sapMSegBBtnMixed");}d.writeClasses();m=l.getWidth();if(m){d.addStyle("width",m);d.writeStyles();}q=l.getTooltip_AsString();if(q){d.writeAttributeEscaped("title",q);}d.writeAttribute("tabindex",l.getEnabled()?"0":"-1");o=l.getTextDirection();if(o!==sap.ui.core.TextDirection.Inherit){d.writeAttribute("dir",o.toLowerCase());}d.writeAccessibilityState(l,{role:"radio",checked:e===l.getId()});if(b&&p!==""){if(g!==""){p+=" "+g;}else{d.writeAttributeEscaped("title",p);}d.writeAttributeEscaped("aria-label",p);}d.write(">");if(h&&b){d.renderControl(b);}if(g!==""){d.write('<div class="sapLudsiSegmentedRendererText">');d.writeEscaped(g,false);d.write("</div>");}}else{d.writeInvisiblePlaceholderData(l);d.write(">");}d.write("</li>");}d.write("</ul>");};return a;});