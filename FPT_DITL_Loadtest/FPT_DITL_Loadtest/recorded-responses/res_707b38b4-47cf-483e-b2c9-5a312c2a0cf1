caf_callback_getAppKind=function(){return"InfoView";};addQueryStringSeperator=function(url){if(url.length==0){return"?";}if(url.indexOf("?")==-1){return url+"?";}else{if("&"!=url.charAt(url.length-1)){return url+"&";}else{return url;}}};getAbsoluteRootContext=function(url,rootContext){var lastIndex=url.indexOf(rootContext);if(lastIndex==-1){lastIndex=url.length;}return url.substring(0,lastIndex);};caf_callback_opendoc=function(url,params,win,lang){var finalUrl=addQueryStringSeperator(url);finalUrl+="appKind="+caf_callback_getAppKind();finalUrl+="&isBILP=true";var warName;var warNameCallback=findCallback("getWarName");if(warNameCallback!=null){warName=warNameCallback();}var service=warName+"/appService.jsp";finalUrl+="&service="+service;if(lang!=null&&lang!=""&&lang!="null"){finalUrl+="&loc="+lang;finalUrl+="&pvl="+lang;}else{finalUrl+="&loc="+getProductLocale();if(getProductViewingLocale()==="in_ID"){finalUrl+="&pvl="+encodeURIComponent("id_ID");}else{finalUrl+="&pvl="+getProductViewingLocale();}}if(params!=null&&params!=""){finalUrl+="&"+params;}var absoluteRootContext=getAbsoluteRootContext(window.location.href,warName);var opendocurl=absoluteRootContext+finalUrl;win.location=opendocurl;};caf_callback_invokeAction=function(url,actionId,objIds,containerId,actionType,actionSetPageType,extraUrlParams,parentWin){var index=url.lastIndexOf("../");if(index>-1){url=url.substring(index+2);}var webapproot=document.location.pathname;webapproot=webapproot.substring(0,webapproot.lastIndexOf("/main.jsp"));webapproot=webapproot.substring(0,webapproot.lastIndexOf("/FioriBI"));if(url.indexOf("/")==0){url=window.location.protocol+"//"+window.location.host+webapproot+url;}else{url=window.location.protocol+"//"+window.location.host+webapproot+"/"+url;}var finalUrl=caf_appendQueryString(url,actionId,objIds,containerId,actionType,actionSetPageType,"standalone",extraUrlParams);if(parentWin!=null&&parentWin!="undefined"){parentWin.location=finalUrl;}else{window.open(finalUrl,"_blank");}};function caf_appendQueryString(url,actionId,objIds,containerId,actionType,actionSetPageType,ctx,extraUrlParams){var warName;var warNameCallback=findCallback("getWarName");if(warNameCallback!=null){warName=warNameCallback();}var service=warName+"/appService.jsp";var finalUrl=addQueryStringSeperator(url);finalUrl+="appKind="+_URIEncode("InfoView");finalUrl+="&service="+_URIEncode(service);finalUrl+="&loc="+_URIEncode(getProductLocale());if(getProductViewingLocale()==="in_ID"){finalUrl+="&pvl="+_URIEncode("id_ID");}else{finalUrl+="&pvl="+_URIEncode(getProductViewingLocale());}finalUrl+="&ctx="+_URIEncode(ctx);if(""!=actionId){finalUrl+="&actId="+_URIEncode(actionId);}if(""!=objIds){finalUrl+="&objIds="+_URIEncode(objIds);}finalUrl+="&containerId="+_URIEncode(containerId);if(extraUrlParams&&extraUrlParams!=""){finalUrl+="&"+extraUrlParams;}return finalUrl;}_URIEncode=function(s){if((typeof(encodeURIComponent)).toLowerCase()=="function"){return encodeURIComponent(s);}else{var tmp=escape(s);return tmp.replace(/\+/g,"%2b");}};