jQuery.sap.declare("com.sap.lumx.caf.view.LUMX_Viewer");com.sap.lumx.caf.view.LUMX_Viewer=function(){this.domain="LUMX";this.lumxHandler=BILaunchpadApp.oClientActionHandler.getClientHandler(this.domain);if(!this.lumxHandler.lumxViewers){this.lumxHandler.lumxViewers=[]}var b=sap.ui.getCore().getEventBus();b.subscribe(BILaunchpadApp.oConstants.PLATFORM_EXTERNAL_CHANNEL,BILaunchpadApp.oConstants.CLOSE_EVENT,this.closeEvent,this)};com.sap.lumx.caf.view.LUMX_Viewer.prototype.init=function(){};com.sap.lumx.caf.view.LUMX_Viewer.prototype.reset=function(){this.eventBus=null;this.lumxHandler=null};com.sap.lumx.caf.view.LUMX_Viewer.prototype.closeEvent=function(h,e,g){if(this.pageId===g.pageId){var f=this.lumxHandler.lumxViewers[g.pageId];this._triggerUnload(f);f.destroyContent()}};com.sap.lumx.caf.view.LUMX_Viewer.prototype._triggerUnload=function(f){var d=f.$().find("iframe");var e=d.contents().find("body");e.trigger("unload")};com.sap.lumx.caf.view.LUMX_Viewer.prototype.getContent=function(j){var p=this;this.pageId=j.pageId;var k=this.domain+j.id+j.pageId;var l=new sap.ui.layout.VerticalLayout({width:"100%",height:"100%"});l.addStyleClass("sapUiSizeCompact");l.addStyleClass("sapUiResponsiveMargin");this.lumxHandler.lumxViewers[j.pageId]=l;jQuery.sap.require("com.sap.lumx.caf.view.LUMX_DataProvider");var n=new com.sap.lumx.caf.view.LUMX_DataProvider();jQuery.sap.require("com.sap.lumx.caf.view.LUMX_ViewerRender");var o=n.checkViewType(j);var i=new com.sap.lumx.caf.view.LUMX_ViewerRender();var m=i.render(j,o,k);p.lumxHandler.lumxViewers[j.pageId].addContent(m);return l};com.sap.lumx.caf.view.LUMX_Viewer.prototype.validate=function(){var b={status:true,message:""};return b};com.sap.lumx.caf.view.LUMX_Viewer.prototype.getData=function(){var b={Format:"LUMX"};return b};