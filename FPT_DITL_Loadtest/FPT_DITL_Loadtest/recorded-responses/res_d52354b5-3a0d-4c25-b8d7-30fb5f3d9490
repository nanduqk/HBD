define("zen.rt.uiframework/cvom_m/js/common/Locale",[],function(){var c={};var a={};c.setLocale=function(){var e=sap.viz.api.env.Locale.get();sap.viz.api.env.Format.useDefaultFormatter(true);var d=sap.ui.getCore().getConfiguration().getFormatLocale();d=d==="zh"?"zh_CN":d;if(d!==e){sap.viz.api.env.Locale.set(d,null,{isLoaded:true});}};c.getBundle=function(d){if(!a[d]){a[d]=jQuery.sap.resources({url:d,locale:b()});}return a[d];};function b(){var d=sap.ui.getCore().getConfiguration().getLanguage();if(d.indexOf("-")>-1){d=d.split("-");d=d[0];}if(d.indexOf("_")>-1){d=d.split("_");d=d[0];}return d==="zh"?"zh_CN":d;}return c;});