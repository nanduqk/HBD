define("zen.rt.components.textfield/resources/js/textfield_handler",["sap/zen/basehandler"],function(BaseHandler){var TextFieldHandler=function(){BaseHandler.apply(this,arguments);var dispatcher=BaseHandler.dispatcher;this.create=function(oChainedControl,oControlProperties,oComponentProperties){var id=oControlProperties.id;var oText=this.createTextWithHeight(id);init(oText,oControlProperties,oComponentProperties);var funclet=function(oEvent){oEvent.stopPropagation();eval(oControlProperties.onclick);};if(oControlProperties.onclick&&oControlProperties.onclick.length>0){oText.attachBrowserEvent("click",funclet);}return oText;};this.update=function(oText,oControlProperties,oComponentProperties){if(oControlProperties){init(oText,oControlProperties,oComponentProperties);}return oText;};function init(oText,oControlProperties,oComponentProperties){var text=dispatcher.getValue(oControlProperties,"text");oText.setCssStyle(oControlProperties.cssStyle);oText.setTooltip(oControlProperties.tooltip);oText.setText(text);if(oControlProperties.onclick!==null&&oControlProperties.onclick!==undefined&&oControlProperties.onclick!=""){oText.addStyleClass("zenClickable");}if(sap.zen.Dispatcher.instance.isMainMode()){oText.addStyleClass("zenTextField"+oControlProperties.style);}else{if(oControlProperties.style==="STANDARD"){oText.setDesign(sap.ui.commons.TextViewDesign.Standard);}else{if(oControlProperties.style==="HEADING1"){oText.setDesign(sap.ui.commons.TextViewDesign.H1);}else{if(oControlProperties.style==="HEADING2"){oText.setDesign(sap.ui.commons.TextViewDesign.H2);}else{if(oControlProperties.style==="HEADING3"){oText.setDesign(sap.ui.commons.TextViewDesign.H3);}else{if(oControlProperties.style==="HEADING4"){oText.setDesign(sap.ui.commons.TextViewDesign.H4);}}}}}}}this.getType=function(){return"textfield";};};return new TextFieldHandler();});