define("zen.rt.components.vizframe/js/referenceLines/common/js/widgets/Dialog",[],function(){jQuery.sap.declare("sap.ludsi.Dialog");jQuery.sap.require("sap.m.Dialog");jQuery.sap.require("sap.m.MessageBox");var b=new sap.ui.model.resource.ResourceModel({bundleUrl:"zen.res/zen.rt.components.vizframe/langs/localization.properties"});var a=b.getResourceBundle();if(sap.ludsi==undefined){sap.ludsi={};}sap.ludsi.Dialog=sap.m.Dialog.extend("sap.ludsi.Dialog",{renderer:{}});sap.ludsi.Dialog.prototype.init=function(){sap.m.Dialog.prototype.init.apply(this,arguments);this.setDraggable(true);this.addStyleClass("sapLudsiDialog");this.addStyleClass("zenDisableAuthoring");};sap.ludsi.Dialog.prototype.onAfterRendering=function(){sap.m.Dialog.prototype.onAfterRendering.apply(this,arguments);};sap.ludsi.Dialog.prototype.exit=function(){sap.m.Dialog.prototype.exit.apply(this,arguments);};sap.ludsi.Dialog.prototype._dialogClose=function(){this.close();};sap.ludsi.Dialog.showError=function(c,d){sap.m.MessageBox.error(c.message,{onClose:d,styleClass:"sapLudsiGenericDialog sapLudsiErrorMessageDialog"});};sap.ludsi.Dialog.showErrorWithLink=function(e,h){var d=new sap.m.Label().setText(e.message);var j=new sap.m.VBox();var i=new sap.m.HBox();var c=new sap.m.Label({text:e.linkLabel});var g=new sap.m.Link({text:e.linkText,target:"_blank",href:e.linkUrl});i.addItem(c).addItem(g);j.addItem(d).addItem(i);var f={title:e.title,layout:j,buttons:[{text:"Close",callback:function(){h("close");}}],icon:e.icon,cssClass:e.cssClass+" sapMMessageBoxError sapLudsiGenericDialog sapLudsiErrorMessageDialog",emphasizedButtonIndex:0};sap.ludsi.Dialog.showGeneric(f);};sap.ludsi.Dialog.showHtml=function(c,e){var g=new sap.m.FormattedText();if(c&&c.message){g.setHtmlText(c.message);}else{g.setHtmlText("No message");}var f=new sap.m.VBox();f.addItem(g);var d={title:c.title,layout:f,buttons:[{text:"Close",callback:function(){e("close");}}],icon:c.icon,cssClass:c.cssClass+" sapLudsiGenericDialog sapLudsiInformationMessageDialog",emphasizedButtonIndex:0};sap.ludsi.Dialog.showGeneric(d);};sap.ludsi.Dialog.showInformation=function(c,d){sap.m.MessageBox.information(c.message,{onClose:d,styleClass:"sapLudsiGenericDialog sapLudsiInformationMessageDialog"});};sap.ludsi.Dialog.showSuccess=function(c,d){sap.m.MessageBox.success(c.message,{onClose:d,styleClass:"sapLudsiGenericDialog sapLudsiSuccessMessageDialog"});};sap.ludsi.Dialog.showWarning=function(c,d){sap.m.MessageBox.warning(c.message,{onClose:d,styleClass:"sapLudsiGenericDialog sapLudsiWarningMessageDialog"});};sap.ludsi.Dialog.confirm=function(c,d){sap.m.MessageBox.confirm(c.message,{onClose:d,styleClass:"sapLudsiGenericDialog sapLudsiConfirmDialog"});};sap.ludsi.Dialog.show=function(c,d){sap.m.MessageBox.alert(c.message,{title:c.title,onClose:d,styleClass:"sapLudsiGenericDialog sapLudsiAlertDialog"});};sap.ludsi.Dialog.showQuestion=function(d,f){var c=new sap.m.Label().setText(d.message);var e={title:d.title,layout:c,buttons:[{text:a.getText("DIALOG_YES"),callback:function(){f("yes");}},{text:a.getText("DIALOG_NO"),callback:function(){f("no");}},{text:a.getText("DIALOG_CANCEL"),callback:function(){f("cancel");}}],icon:d.icon,cssClass:d.cssClass,emphasizedButtonIndex:0};sap.ludsi.Dialog.showGeneric(e);};sap.ludsi.Dialog.showGeneric=function(c){var e=new sap.ludsi.Dialog();if(c.title){e.setTitle(c.title);}else{e.setTitle(a.getText("DIALOG_DEFAULT_TITLE"));}if(c.icon){e.setIcon(c.icon);}if(c.cssClass){var d=c.cssClass.split(" ");d.forEach(function(f){if(f!==""){e.addStyleClass(f);}});}if(c.layout){e.addContent(c.layout);}else{e.addContent(new sap.m.Label({text:a.getText("DIALOG_NO_CONTENT")}));}c.buttons.forEach(function(f){var g=new sap.m.Button();g.setText(f.text);g.attachPress(function(h){e.close();f.callback(h);});if(c.buttons.indexOf(f)===(c.emphasizedButtonIndex?c.emphasizedButtonIndex:0)){g.setType(sap.m.ButtonType.Emphasized);}e.addButton(g);});e.open();};sap.ludsi.Dialog.showInfoWithCheckbox=function(k,e,g){var h=new sap.m.Text().setText(k.message);var f=new sap.m.VBox();var i=new sap.m.HBox();var j=new sap.m.CheckBox({selected:false,select:function(l){e(l.mParameters.selected);}.bind(this),});var d=new sap.m.Label({text:k.checkBoxLabel});i.addItem(j).addItem(d);f.addItem(h).addItem(i);var c={title:k.title,layout:f,buttons:[{text:"OK",callback:function(){g("OK");}}],cssClass:k.cssClass+" sapMMessageBoxError sapLudsiGenericDialog sapLudsiErrorMessageDialog",emphasizedButtonIndex:0};sap.ludsi.Dialog.showGeneric(c);};return sap.ludsi.Dialog;});